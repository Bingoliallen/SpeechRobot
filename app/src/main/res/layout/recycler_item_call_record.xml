<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:paddingBottom="15dp"
  android:background="@color/common_app_theme2">

  <TextView
    android:id="@+id/tv_mobile"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="15dp"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:ellipsize="end"
    android:maxLines="2"
    android:textColor="@color/common_app_text_theme2"
    android:textSize="@dimen/text_size4"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toLeftOf="@+id/tv_result"
    app:layout_constraintTop_toTopOf="parent" />

  <TextView
    android:id="@+id/tv_result"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginRight="15dp"
    android:textSize="@dimen/text_size5"
    app:layout_constraintBottom_toBottomOf="@+id/tv_mobile"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="@+id/tv_mobile" />

  <TextView
    android:id="@+id/tv_create"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="17dp"
    android:layout_marginLeft="15dp"
    android:textColor="@color/common_app_text_theme3"
    android:textSize="@dimen/text_size6"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/tv_mobile" />

  <TextView
    android:id="@+id/tv_level"
    android:layout_width="wrap_content"
    android:layout_height="22.5dp"
    android:layout_marginTop="17dp"
    android:layout_marginLeft="15dp"
    android:gravity="center"
    android:minWidth="50dp"
    android:textColor="@color/common_app_text_theme4"
    android:textSize="@dimen/text_size8"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/tv_create" />

  <TextView
    android:id="@+id/tv_count"
    android:layout_width="wrap_content"
    android:layout_height="22.5dp"
    android:layout_marginTop="17dp"
    android:layout_marginLeft="15dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:background="@drawable/round5_rect_app_theme6"
    android:gravity="center"
    android:minWidth="50dp"
    android:textColor="@color/common_app_text_theme3"
    android:textSize="@dimen/text_size8"
    app:layout_constraintLeft_toRightOf="@+id/tv_level"
    app:layout_constraintTop_toBottomOf="@+id/tv_create" />

  <TextView
    android:id="@+id/tv_time"
    android:layout_width="wrap_content"
    android:layout_height="22.5dp"
    android:layout_marginTop="17dp"
    android:layout_marginLeft="15dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    android:background="@drawable/round5_rect_app_theme6"
    android:gravity="center"
    android:minWidth="50dp"
    android:textColor="@color/common_app_text_theme3"
    android:textSize="@dimen/text_size8"
    app:layout_constraintLeft_toRightOf="@+id/tv_count"
    app:layout_constraintTop_toBottomOf="@+id/tv_create" />

  <TextView
    android:id="@+id/tv_call_num"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginRight="15dp"
    android:drawablePadding="5dp"
    android:drawableRight="@drawable/call"
    android:textColor="@color/common_app_text_theme1"
    android:textSize="@dimen/text_size5"
    app:layout_constraintBottom_toBottomOf="@+id/tv_level"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="@+id/tv_level" />

</android.support.constraint.ConstraintLayout>